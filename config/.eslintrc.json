{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": [
      "prettier",
      "plugin:react/recommended",
      "airbnb"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12
    },
    "plugins": [
      "prettier",
      "react",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  }